{
  "name": "baeklog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@next/bundle-analyzer": "^12.2.3",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "next": "^12.2.3",
    "next-redux-wrapper": "^7.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-spinners": "^0.13.3",
    "redux-saga": "^1.1.3",
    "tailwind-merge": "^1.5.1",
    "typesafe-actions": "^5.1.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "dev": "next dev",
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "generate:openapi": "openapi-generator-cli generate --generator-key v1.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.3",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.18.0",
    "eslint-config-next": "^12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "husky": ">=7",
    "lint-staged": ">=10",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.5"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "plugin:prettier/recommended",
      "plugin:tailwindcss/recommended"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "settings": {
      "tailwindcss": {
        "callees": [
          "clsxm"
        ]
      }
    },
    "rules": {
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error"
    }
  },
  "eslintIgnore": [
    "generated"
  ],
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "printWidth": 80,
    "arrowParens": "always",
    "endOfLine": "auto",
    "semi": true,
    "useTabs": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^16.x",
    "npm": "please-use-yarn",
    "yarn": "^1.22.x"
  },
  "lint-staged": {
    "*.json": [
      "prettier --write"
    ],
    ".*rc": [
      "prettier --write"
    ],
    "src/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
